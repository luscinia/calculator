import java.util.Scanner;
public class Main {
public static void main(String[]args){
    int a,b,c,z;
   
    Scanner scan=new Scanner(System.in);
    z= scan.nextInt();
   
    while(z<=999&z>=100)
    {
    a=z;
    b=z0/10;
    c=z/100;
    if (a*a*a+b*b*b+c*c*c==z)
    System.out.println("Yes");
    else System.out.println("No");
    z= scan.nextInt();
    }
}
}
